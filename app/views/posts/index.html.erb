<% content_for :javascripts do %>
  <%= javascript_include_tag 'jquery1.5.1.min',"../jquery-syntax/jquery.syntax.min","jquery-ui-1.8.10.custom.min","application", :cache => true %>
  <%= javascript_tag do %>
    var id_max = <%=@id_max%>;
    var titles = <%=raw @titles%>;
  <%end%>
<%end%>

<div id="all">

  <div class='box' id="header">
    <div id="banner"></div>
    <div id="slider"></div>
    <div id="slider_subtitle">
      <span id="title"></span>
      <span id="warning">Arraste para navegar pelos posts</span>
    </div>
  </div>

  <div id="post">This Will Be Replaced By Post Div via AJAX</div>
  
</div>
